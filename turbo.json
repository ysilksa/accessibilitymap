{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "install:deps": {
      "dependsOn": [
        "^install:deps"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "format": {
      "dependsOn": [
        "^format"
      ]
    },
    "format:check": {
      "dependsOn": [
        "^format:check"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "^lint:fix"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
